<h1>Edit Format</h1>

<%= form_with(model: @format) do |form| %>

  <p>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </p>

    <p>
    <%= form.label :multiline %>
    <%= form.check_box :multiline %>
  </p>

  <p>
    <%= form.label :stand %>
    <%= form.date_field :stand %>
  </p>


  <p>
    <%= form.submit %>
  </p>

<% end %>

<%= link_to 'Back', "/formats" %>